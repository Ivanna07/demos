#message service GET
curl --user root:root -H Content-Type:application/json http://localhost:8080/rest/api/messages/send?userName=root
curl --user root:root -H Content-Type:application/json http://localhost:8080/rest/api/messages/received?userName=root

#user service GET
curl --user root:root -H Content-Type:application/json http://localhost:8080/rest/api/users?userName=root

#user service PUT
curl -X PUT --user tux:tux -H Content-Type:application/json http://localhost:8080/rest/api/users -d @json_samples/users_put.json
curl -X PUT --user root:root -H Content-Type:application/json http://localhost:8080/rest/api/users -d @json_samples/users_put_root.json

#user service POST
curl -X POST  --user root:root -H Content-Type:application/json http://localhost:8080/rest/api/users -d @json_samples/users_post.json


#PUT invalid
curl -X PUT --user tux:tux -H Content-Type:application/json http://localhost:8080/rest/api/users -d @json_samples/users_put_root.json


